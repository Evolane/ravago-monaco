{
    "enabled": true,
    "name": "{{.appname}}",
    "customDescription": null,
    "metricName": "{{.metricname}}_apdex",
    "metricExpression": "(100)*(builtin:apps.web.actionCount.category:filter(eq(\"Apdex category\",SATISFIED)):splitBy())/(builtin:apps.web.actionCount.category:splitBy())",
    "evaluationType": "AGGREGATE",
    "filter": "type(\"APPLICATION\"),entityName(\"{{.appname}}\")",
    "evaluationWindow": "-1w",
    "targetSuccess": 90,
    "targetWarning": 91,
    "errorBudgetBurnRate": {
        "burnRateVisualizationEnabled": true,
        "fastBurnThreshold": 10
    }
}